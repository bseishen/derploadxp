id=sl_svn25_288

><td class="source">/*<br></td></tr
><tr
id=sl_svn25_289

><td class="source">Get current values (input register and eeprom) of mcp4728<br></td></tr
><tr
id=sl_svn25_290

><td class="source">*/<br></td></tr
><tr
id=sl_svn25_291

><td class="source">void mcp4728::getStatus()<br></td></tr
><tr
id=sl_svn25_292

><td class="source">{<br></td></tr
><tr
id=sl_svn25_293

><td class="source">  Wire.requestFrom(int(_dev_address), 24);<br></td></tr
><tr
id=sl_svn25_294

><td class="source">  while(Wire.available())<br></td></tr
><tr
id=sl_svn25_295

><td class="source">  { <br></td></tr
><tr
id=sl_svn25_296

><td class="source">    int deviceID = Wire.read();<br></td></tr
><tr
id=sl_svn25_297

><td class="source">    int hiByte = Wire.read();<br></td></tr
><tr
id=sl_svn25_298

><td class="source">    int loByte = Wire.read();<br></td></tr
><tr
id=sl_svn25_299

><td class="source"><br></td></tr
><tr
id=sl_svn25_300

><td class="source">    int isEEPROM = (deviceID &amp; 0B00001000) &gt;&gt; 3;<br></td></tr
><tr
id=sl_svn25_301

><td class="source">    int channel = (deviceID &amp; 0B00110000) &gt;&gt; 4;<br></td></tr
><tr
id=sl_svn25_302

><td class="source">    if (isEEPROM == 1) {<br></td></tr
><tr
id=sl_svn25_303

><td class="source">      _intVrefEp[channel] = (hiByte &amp; 0B10000000) &gt;&gt; 7;<br></td></tr
><tr
id=sl_svn25_304

><td class="source">      _gainEp[channel] = (hiByte &amp; 0B00010000) &gt;&gt; 4;<br></td></tr
><tr
id=sl_svn25_305

><td class="source">      _powerDownEp[channel] = (hiByte &amp; 0B01100000) &gt;&gt; 5;<br></td></tr
><tr
id=sl_svn25_306

><td class="source">      _valuesEp[channel] = word((hiByte &amp; 0B00001111), loByte);<br></td></tr
><tr
id=sl_svn25_307

><td class="source">    }<br></td></tr
><tr
id=sl_svn25_308

><td class="source">    else {<br></td></tr
><tr
id=sl_svn25_309

><td class="source">      _intVref[channel] = (hiByte &amp; 0B10000000) &gt;&gt; 7;<br></td></tr
><tr
id=sl_svn25_310

><td class="source">      _gain[channel] = (hiByte &amp; 0B00010000) &gt;&gt; 4;<br></td></tr
><tr
id=sl_svn25_311

><td class="source">      _powerDown[channel] = (hiByte &amp; 0B01100000) &gt;&gt; 5;<br></td></tr
><tr
id=sl_svn25_312

><td class="source">      _values[channel] = word((hiByte &amp; 0B00001111), loByte);<br></td></tr
><tr
id=sl_svn25_313

><td class="source">    }<br></td></tr
><tr
id=sl_svn25_314

><td class="source">  }<br></td></tr
><tr
id=sl_svn25_315

><td class="source">}<br></td></tr
><tr
id=sl_svn25_316

><td class="source">/*<br></td></tr
><tr
id=sl_svn25_317

><td class="source">FastWrite input register values - All DAC ouput update. refer to DATASHEET 5.6.1<br></td></tr
><tr
id=sl_svn25_318

><td class="source">DAC Input and PowerDown bits update.<br></td></tr
><tr
id=sl_svn25_319

><td class="source">No EEPROM update<br></td></tr
><tr
id=sl_svn25_320

><td class="source">*/<br></td></tr
><tr
id=sl_svn25_321

><td class="source">uint8_t mcp4728::fastWrite() {<br></td></tr
><tr
id=sl_svn25_322

><td class="source">  Wire.beginTransmission(_dev_address);<br></td></tr
><tr
id=sl_svn25_323

><td class="source">  for (uint8_t channel=0; channel &lt;= 3; channel++) {<br></td></tr
><tr
id=sl_svn25_324

><td class="source">    Wire.write(highByte(_values[channel]));<br></td></tr
><tr
id=sl_svn25_325

><td class="source">    Wire.write(lowByte(_values[channel]));<br></td></tr
><tr
id=sl_svn25_326

><td class="source">  }<br></td></tr
><tr
id=sl_svn25_327

><td class="source">  return Wire.endTransmission();<br></td></tr
><tr
id=sl_svn25_328

><td class="source">}<br></td></tr
><tr
id=sl_svn25_329

><td class="source">/*<br></td></tr
><tr
id=sl_svn25_330

><td class="source">MultiWrite input register values - All DAC ouput update. refer to DATASHEET 5.6.2<br></td></tr
><tr
id=sl_svn25_331

><td class="source">DAC Input, Gain, Vref and PowerDown bits update<br></td></tr
><tr
id=sl_svn25_332

><td class="source">No EEPROM update<br></td></tr
><tr
id=sl_svn25_333

><td class="source">*/<br></td></tr
><tr
id=sl_svn25_334

><td class="source">uint8_t mcp4728::multiWrite() {<br></td></tr
><tr
id=sl_svn25_335

><td class="source">  Wire.beginTransmission(_dev_address);<br></td></tr
><tr
id=sl_svn25_336

><td class="source">  for (uint8_t channel=0; channel &lt;= 3; channel++) {<br></td></tr
><tr
id=sl_svn25_337

><td class="source">    Wire.write(MULTIWRITE | (channel &lt;&lt; 1)); <br></td></tr
><tr
id=sl_svn25_338

><td class="source">    Wire.write(_intVref[channel] &lt;&lt; 7 | _powerDown[channel] &lt;&lt; 5 | _gain[channel] &lt;&lt; 4 | highByte(_values[channel]));<br></td></tr
><tr
id=sl_svn25_339

><td class="source">    Wire.write(lowByte(_values[channel]));<br></td></tr
><tr
id=sl_svn25_340

><td class="source">  }<br></td></tr
><tr
id=sl_svn25_341

><td class="source">  return Wire.endTransmission();<br></td></tr
><tr
id=sl_svn25_342

><td class="source">}<br></td></tr
><tr
id=sl_svn25_343

><td class="source">/*<br></td></tr
><tr
id=sl_svn25_344

><td class="source">SingleWrite input register and EEPROM - a DAC ouput update. refer to DATASHEET 5.6.4<br></td></tr
><tr
id=sl_svn25_345

><td class="source">DAC Input, Gain, Vref and PowerDown bits update<br></td></tr
