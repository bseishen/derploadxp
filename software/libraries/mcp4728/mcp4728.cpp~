id=sl_svn25_349

><td class="source">  Wire.beginTransmission(_dev_address);<br></td></tr
><tr
id=sl_svn25_350

><td class="source">  Wire.write(SINGLEWRITE | (channel &lt;&lt; 1)); <br></td></tr
><tr
id=sl_svn25_351

><td class="source">  Wire.write(_intVref[channel] &lt;&lt; 7 | _powerDown[channel] &lt;&lt; 5 | _gain[channel] &lt;&lt; 4 | highByte(_values[channel]));<br></td></tr
><tr
id=sl_svn25_352

><td class="source">  Wire.write(lowByte(_values[channel]));<br></td></tr
><tr
id=sl_svn25_353

><td class="source">  return Wire.endTransmission();<br></td></tr
><tr
id=sl_svn25_354

><td class="source">}<br></td></tr
><tr
id=sl_svn25_355

><td class="source">/*<br></td></tr
><tr
id=sl_svn25_356

