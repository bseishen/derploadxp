><td class="source">#define mcp4728_h<br></td></tr
><tr
id=sl_svn25_7

><td class="source"><br></td></tr
><tr
id=sl_svn25_8

><td class="source">#include &quot;Arduino.h&quot;<br></td></tr
><tr
id=sl_svn25_9

><td class="source">#include &lt;Wire.h&gt;<br></td></tr
><tr
id=sl_svn25_10

><td class="source"><br></td></tr
><tr
id=sl_svn25_11

><td class="source">#define defaultVDD 5000<br></td></tr
><tr
id=sl_svn25_12

><td class="source">#define BASE_ADDR 0x60<br></td></tr
><tr
id=sl_svn25_13

><td class="source">#define RESET 0B00000110<br></td></tr
><tr
id=sl_svn25_14

><td class="source">#define WAKE 0B00001001<br></td></tr
><tr
id=sl_svn25_15

><td class="source">#define UPDATE 0B00001000<br></td></tr
><tr
id=sl_svn25_16

><td class="source">#define MULTIWRITE 0B01000000<br></td></tr
><tr
id=sl_svn25_17

><td class="source">#define SINGLEWRITE 0B01011000<br></td></tr
><tr
id=sl_svn25_18

><td class="source">#define SEQWRITE 0B01010000<br></td></tr
><tr
id=sl_svn25_19

><td class="source">#define VREFWRITE 0B10000000<br></td></tr
><tr
id=sl_svn25_20

><td class="source">#define GAINWRITE 0B11000000<br></td></tr
><tr
id=sl_svn25_21

><td class="source">#define POWERDOWNWRITE 0B10100000<br></td></tr
><tr
id=sl_svn25_22

><td class="source">#define GENERALCALL 0B0000000<br></td></tr
><tr
id=sl_svn25_23

><td class="source">#define GAINWRITE 0B11000000<br></td></tr
><tr
id=sl_svn25_24

><td class="source"><br></td></tr
